{
  "name": "surrealql",
  "displayName": "surrealql",
  "description": "Language support for SurrealQL",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:surrealql.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "surrealql",
        "extensions": [
          ".surql"
        ],
        "aliases": [
          "surrealql",
          "surrealdb"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "surrealql",
        "scopeName": "source.surealql",
        "path": "./syntaxes/surrealql.json"
      },
      {
        "injectTo": [
          "source.js",
          "source.js.jsx",
          "source.jsx",
          "source.ts",
          "source.tsx",
          "source.vue",
          "text.html.markdown"
        ],
        "scopeName": "inline.surrealql-lit",
        "path": "./syntaxes/surrealql-lit.json",
        "embeddedLanguages": {
          "meta.embedded.block.surrealql": "surrealql"
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.71.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "vscode-languageclient": "^8.0.2"
  }
}
